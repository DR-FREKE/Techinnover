{
  "name": "techinnover",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.2"
    },
  "description": "techininover project",
  "main": "app.js",
  "scripts": {
    "start": "node start.js",
    "server": "nodemon start.js",
    "m-build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild":"npm run install-client && npm run m-build",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "Solomon Ndifereke",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-polyfill": "^6.26.0",
    "nodemon": "^2.0.6"
  }
}
